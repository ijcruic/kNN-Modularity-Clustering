import numpy as np, networkx as nx, os, kNN_Modularity
from matplotlib import pyplot


affinity_test =np.random.rand(1000,1000)
kNetwork = kNN_Modularity.kNetwork()

groups, k , modularity, graph = kNetwork.fit_predict(affinity_test)

print(groups)
print('\n')
print('Best Modularity: {}'.format(modularity))
nx.draw_networkx(graph)
